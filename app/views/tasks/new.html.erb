<h1> Create a New task<h1> 

<hr> 

<%= form_for(@task) do |f| %>
    <%= f.label :project
    <%= f.collection_select :project_id, Project.all, :id, :name %> 

    <%= form_for(@task) do |f| %>
        <%= f.label :programmer
        <%= f.collection_select :programmer_id, Programmer.all, :id, :name %>     

    <%= f.submit "Deploy More Labs" %>

<% end %>


